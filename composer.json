{
    "name": "shapecode/git-tools",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Nikita Loges",
            "email": "git@nikita-loges.de"
        }
    ],
    "autoload": {
        "psr-4": {
            "": "src/"
        }
    },
    "require": {
        "php": "~5.6|~7.0",

        "symfony/console": "~2.8|~3.0",
        "symfony/var-dumper": "~2.8|~3.0",
        "symfony/debug": "~2.8|~3.0",
        "symfony/finder": "~2.8|~3.0",

        "symfony/filesystem": "~2.8",
        "symfony/options-resolver": "~2.8",
        "symfony/process": "~2.8",

        "jbzoo/utils": "~1.7",

        "kzykhys/git": "~0.1"
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}